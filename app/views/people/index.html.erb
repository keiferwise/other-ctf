
<h1>PEOPLE</h1>


<table>
  <tr>
    <th>First</th>
    <th>Last</th>
    <th></th>
  </tr>
 
  <% @people.each do |person| %>
    <tr>
      <td><%= person.first_name %></td>
      <td><%= person.last_name %></td>
      <td><%= link_to 'Show', person_path(person),class:"table-button-single" %></td>
    </tr>
  <% end %>
</table>